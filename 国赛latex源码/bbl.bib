	author = {邓, 立宝 and 吴, 怡然 and 郭, 苏},
	year = {2020},
	note = {2 citations(CNKI)[2023-9-10]{\textless}北大核心{\textgreater}},
	keywords = {定日镜场布局, 多目标优化, 反向学习, 归一化 multi-objective optimization, 佳点集, 塔式太阳能发电, good-point set, heliostat field layout, MOEA/D, normalization, opposition-based learning, solar power tower system},
	pages = {37--43},
