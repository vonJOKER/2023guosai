	keywords = {成本 solar power tower plant, 尺寸, 定日镜, 塔式电站, cost, heliostat, size},
	pages = {199--203},
	file = {塔式电站中定日镜尺寸的选择_张宏丽.pdf:C\:\\Users\\yuhangmeng\\Zotero\\storage\\K3ZL7CE7\\塔式电站中定日镜尺寸的选择_张宏丽.pdf:application/pdf},
